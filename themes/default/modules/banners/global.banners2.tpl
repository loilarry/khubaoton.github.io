<!-- BEGIN: loop -->
<div class="nv-block-banners col-lg-8 col-md-8 fix-more-padding" style="height:350px;">
    <!-- BEGIN: type_image_link -->
    <a rel="nofollow" href="{DATA.link}" onclick="this.target='{DATA.target}'" title="{DATA.file_alt}"><img alt="{DATA.file_alt}" src="{DATA.file_image}" width="{DATA.file_width}"
    style="overflow: hidden;
    background-color: #fff;
    height: 200px;
    display: inline-block;
    "><td>
     <div class="col-sm-8 col-md-24" style="text-align:center;">
     <h1 style="line-height: 30px; font-size: 18px !important;
    text-transform: uppercase !important;color: #337ab7;">{DATA.title}</h1></a>
      {DATA.file_alt}</div>
    <!-- END: type_image_link -->
    <!-- BEGIN: type_image -->
    <img alt="{DATA.file_alt}" src="{DATA.file_image}" width="{DATA.file_width}">
   
    <!-- END: type_image -->
    <!-- BEGIN: bannerhtml -->
    <div class="clearfix text-left">
        {DATA.bannerhtml}
    </div>
    <!-- END: bannerhtml -->
</div>
<!-- END: loop -->
<!-- END: main -->
